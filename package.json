{
  "name": "codegraph",
  "displayName": "CodeGraph",
  "description": "Code Visualize",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.108.1"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:codegraph.open"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "codegraph.open",
        "title": "CodeGraph: Open"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test",
    "build:webview": "cd webview-ui && npm run build",
    "copy:webview": "node ./scripts/copy-webview.mjs",
    "build:all": "npm run build:webview && npm run copy:webview && npm run compile"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.10",
    "@types/node": "22.x",
    "@types/vscode": "^1.108.1",
    "@vscode/test-cli": "^0.0.12",
    "@vscode/test-electron": "^2.5.2",
    "eslint": "^9.39.2",
    "typescript-eslint": "^8.52.0"
  },
  "dependencies": {
    "typescript": "^5.9.3"
  }
}
